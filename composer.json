    "paragonie/constant_time_encoding": "^3.0.0",
    "php-curl-class/php-curl-class": "^11.1.0",
    "statink/yii2-anonymizer": "^1.0.4",
    "statink/yii2-bukiicons": "^1.0.1",
    "statink/yii2-cal-heatmap": "^2.1.1",
    "statink/yii2-ip-badge": "^1.0.4",
    "statink/yii2-jdenticon": "^1.0.1",
    "statink/yii2-momentjs": "^1.0",
    "statink/yii2-sortable-table": "^1.0.2",
    "statink/yii2-spl1-stages": "^1.0.1",
    "statink/yii2-spl2-stages": "^1.0.2",
    "statink/yii2-twitter-webintents": "^1.0.2",
    "symfony/polyfill-php83": "^1.32",
    "symfony/yaml": "^7.2.6",
    "twbs/bootstrap-icons": "^1.12.1",
    "yiisoft/yii2": "^2.0.52",
    "yiisoft/yii2-bootstrap": "^2.0.11",
    "yiisoft/yii2-httpclient": "^2.0.16",
    "yiisoft/yii2-queue": "^2.3.7",
    "yiisoft/yii2-symfonymailer": "^4.0.0"
  },
  "require-dev": {
    "codeception/codeception": ">=5.3.0",
    "codeception/module-asserts": ">=3.2",
    "codeception/module-filesystem": ">=3.0.1",
    "codeception/module-yii2": ">=1.1.12",
    "codeception/specify": ">=2",
    "codeception/verify": ">=3.3",
    "ergebnis/composer-normalize": "^2.47.0",
    "jp3cki/coding-standard": "^1.0.2",
    "nikic/php-parser": "^4.19.4 || ^5.4.0",
    "proget-hq/phpstan-yii2": "^0.8.0",
    "sebastian/diff": "^4.0.6 || ^5.0.0 || ^6.0.0",
    "squizlabs/php_codesniffer": "^3.12.2",
    "yiisoft/yii2-debug": "^2.1.26",
    "yiisoft/yii2-gii": "^2.2.7"
  },
  "replace": {
    "arokettu/random-polyfill": "*",
    "bower-asset/bootstrap": "^3.4.1",
    "bower-asset/eonasdan-bootstrap-datetimepicker": "*",
    "bower-asset/flot": "~0.8.3",
    "bower-asset/inputmask": "*",
    "bower-asset/jquery": "*",
    "bower-asset/jquery-color": "*",
    "bower-asset/moment": "*",
    "bower-asset/moment-timezone": "*",
    "bower-asset/punycode": "*",
    "bower-asset/yii2-pjax": "*",
    "bower-asset/zxcvbn": "*",
    "npm-asset/bootstrap-notify": "^3.1.3",
    "npm-asset/bootswatch": "^3.4.1",
    "npm-asset/cal-heatmap": "*",
    "npm-asset/dseg": "*",
    "npm-asset/emojify.js": "*",
    "npm-asset/flag-icon-css": "*",
    "npm-asset/flag-icons": "*",
    "npm-asset/jdenticon": "*",
    "npm-asset/jquery": "*",
    "npm-asset/jquery-smooth-scroll": "*",
    "npm-asset/moment": "*",
    "npm-asset/moment-timezone": "*",
    "npm-asset/photoswipe": "*",
    "npm-asset/photoswipe-simplify": "*",
    "npm-asset/popperjs--core": "*",
    "npm-asset/rpg-awesome": "*",
    "npm-asset/stupid-table-plugin": "*",
    "symfony/polyfill-php54": "*",
    "symfony/polyfill-php55": "*",
    "symfony/polyfill-php56": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "repositories": {
    "private": {
      "type": "composer",
      "url": "https://satis.stat.ink",
      "exclude": [
        "statink/yii2-cal-heatmap",
        "statink/yii2-paintball"
      ]
    },
    "heatmap": {
      "type": "vcs",
      "url": "https://github.com/statink/yii2-cal-heatmap.git"
    }
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "app\\": "."
    }
  },
  "config": {
    "allow-plugins": {
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "yiisoft/yii2-composer": true
    },
    "platform": {
      "php": "8.2.28"
    },
    "process-timeout": 1800
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    },
    "patches": {
      "lusitanian/oauth": {
        "array_walk interface for PHP 8": "data/patch/lusitanian-oauth-array_walk.patch"
      },
      "yiisoft/yii2": {
        "enhance: callable filter column to GridView": "data/patch/yii2-gridview-callable-filter.patch",
        "enhance: make `protected` CacheController::findCaches()": "data/patch/yii2-cache-command.patch",
        "enhance: use sha256 to caching key": "data/patch/yii2-cache-key.patch"
      },
      "yiisoft/yii2-bootstrap": {
        "non-progress progressbar": "data/patch/yii2-bootstrap-non-progress-progressbar.patch"
      }
    }
  },
  "scripts": {
    "post-install-cmd": [
      "bin/composer-postinstall.sh"
    ],
    "post-update-cmd": [
      "bin/composer-postinstall.sh"
    ]
  }
}
